{
  "name": "personal-website",
  "description": "A personal website developed using Gatsby",
  "version": "1.0.0",
  "author": "Jason Tee <cheeguantee@gmail.com>",
  "dependencies": {
    "babel-plugin-styled-components": "^1.11.1",
    "gatsby": "2.24.47",
    "gatsby-plugin-manifest": "2.4.27",
    "gatsby-plugin-offline": "3.2.26",
    "gatsby-plugin-postcss": "2.3.11",
    "gatsby-plugin-react-helmet": "3.3.10",
    "gatsby-plugin-sharp": "2.6.31",
    "gatsby-plugin-styled-components": "^3.3.10",
    "gatsby-plugin-typography": "^2.5.10",
    "hover.css": "^2.3.2",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.1.0",
    "react-typography": "^0.16.19",
    "styled-components": "^5.1.1",
    "typography": "^0.16.19",
    "typography-theme-funston": "^0.16.19"
  },
  "keywords": [
    "gatsby",
    "tailwind",
    "tailwindcss",
    "purgecss"
  ],
  "scripts": {
    "analyze:lint": "eslint --ext .jsx --ext .js .",
    "analyze:prettier": "prettier --list-different \"**/*.{css,js,jsx,json,md}\"",
    "analyze": "npm run analyze:lint && npm run analyze:prettier",
    "fix:lint": "eslint --ext .jsx --ext .js . --fix",
    "fix:prettier": "prettier --write \"**/*.{css,js,jsx,json,md}\"",
    "fix": "npm run fix:lint && npm run fix:prettier",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "dev": "npm run develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean"
  },
  "devDependencies": {
    "@tailwindcss/custom-forms": "0.2.1",
    "autoprefixer": "9.8.6",
    "babel-eslint": "10.1.0",
    "cssnano": "4.1.10",
    "eslint": "7.8.1",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-react": "7.20.6",
    "gatsby-plugin-eslint": "2.0.8",
    "prettier": "2.1.1",
    "tailwindcss": "1.7.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AmplifiedHuman/personal-website"
  }
}
